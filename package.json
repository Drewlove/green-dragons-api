{
  "name": "green-dragons-api",
  "version": "1.0.0",
  "description": "API for green dragons",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "start": "node src/server.js",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [],
  "engines": {
    "node": "11.9.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.3",
    "helmet": "^3.22.0",
    "knex": "^0.19.5",
    "morgan": "^1.10.0",
    "pg": "^7.18.2",
    "uuid": "^3.4.0",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "postgrator-cli": "^3.3.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drewlove/bookmarks-server.git"
  },
  "bugs": {
    "url": "https://github.com/Drewlove/bookmarks-server/issues"
  },
  "homepage": "https://github.com/Drewlove/bookmarks-server#readme"
}
